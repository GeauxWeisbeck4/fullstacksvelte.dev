{
    "name": "svelte-storybook-tailwind",
    "author": "Jerric Lyns John",
    "license": "MIT",
    "version": "0.0.2",
    "scripts": {
        "format": "prettier --check .",
        "format:fix": "prettier --write .",
        "prelint": "npm run format:fix",
        "lint": "eslint --ext .cjs,.js,.ts,.json,.svelte .",
        "lint:fix": "eslint --fix --ext .cjs,.js,.ts,.json,.svelte .",
        "build": "NODE_ENV=production rollup -c",
        "serve": "NODE_ENV=production rollup -c && live-server ./public",
        "dev": "rollup -c -w",
        "start": "sirv public",
        "storybook": "start-storybook -p 6006 -s ./.storybook/favicons",
        "build-storybook": "build-storybook",
        "static-stories": "build-storybook -c .storybook -o .out"
    },
    "dependencies": {
        "sirv-cli": "^1.0.0"
    },
    "devDependencies": {
        "@babel/core": "^7.12.16",
        "@rollup/plugin-commonjs": "^17.1.0",
        "@rollup/plugin-node-resolve": "^11.1.1",
        "@storybook/addon-actions": "^6.1.18",
        "@storybook/addon-essentials": "^6.1.18",
        "@storybook/addon-links": "^6.1.18",
        "@storybook/svelte": "^6.1.18",
        "@storybook/addons": "^6.1.11",
        "@storybook/theming": "^6.1.11",
        "autoprefixer": "^10.2.4",
        "babel-loader": "^8.2.2",
        "cssnano": "^4.1.10",
        "eslint": "^7.20.0",
        "eslint-config-prettier": "^7.2.0",
        "eslint-plugin-prettier": "^3.3.1",
        "eslint-plugin-svelte3": "^3.0.0",
        "npm-run-all": "^4.1.5",
        "postcss": "^8.2.6",
        "postcss-import": "^14.0.0",
        "postcss-load-config": "^3.0.1",
        "postcss-url": "^10.1.1",
        "prettier": "^2.2.1",
        "prettier-plugin-svelte": "^2.1.4",
        "rollup": "^2.39.0",
        "rollup-plugin-css-only": "^3.1.0",
        "rollup-plugin-livereload": "^2.0.0",
        "rollup-plugin-postcss": "^3.1.3",
        "rollup-plugin-svelte": "^7.1.0",
        "rollup-plugin-terser": "^7.0.2",
        "stylelint": "^13.10.0",
        "stylelint-config-standard": "^20.0.0",
        "svelte": "^3.32.3",
        "svelte-loader": "^3.0.0",
        "svelte-preprocess": "^4.6.8",
        "tailwindcss": "^2.0.3"
    }
}
